void printiln(int x)
{
    printi(x);printc('\n');
}
int gcd(int a, int b)
{
    while(a != b)
    {
        if (a > b)
            a = a - b;
        else
            b = b -a;
    }
    return a;
}

int main()
{
    printiln(gcd(7, 63));
    printiln(gcd(2, 18));
    printiln(gcd(3, 15));
    printiln(gcd(1, 1));
    printiln(gcd(100, 150));
}