void printiln(int x)
{
    printi(x);printc('\n');
}

int fib(int x)
{
    if (x < 2)
    {
        return 1;
    }

    return fib(x - 1) + fib(x - 2);
}

int main()
{
    printiln(fib(0));
    printiln(fib(1));
    printiln(fib(2));
    printiln(fib(3));
    printiln(fib(4));
    printiln(fib(5));    
}
