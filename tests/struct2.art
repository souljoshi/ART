/* Testing basic scoping Rules */
void printiln(int x)
{
    printiln(x);printc('\n');
}
int z;

struct first { int z; }
struct second { int y; }

first::first(int b) { z = b; }
second::second(int a) { y = a; }

void first::member()
{
        z = 0;
}

void second::member()
{
        int y;
        y = 100;
        z = 10;
}

int main()
{
        struct first x;
        struct second y;

        printiln(z);
        printc('\n');

        x = first(-10);
        y = second(-12);

        printiln(z);
        printiln(x.z);
        printiln(y.y);
        printc('\n');

        x.member();
        y.member();

        printiln(z);
        printiln(x.z);
        printiln(y.y);
}
