/* Float Condition Testing */
void printiln(int f){printi(f);printc('\n');}
int main()
{
    double x = 1.2;
    double y = -1.3;
    double z = 1.2;
    double w = 30e7;

    printiln( x == z);
    printiln( x != y);
    printiln( x >= x);
    printiln( x <= z);
    printiln( w > z);
    printiln( y < z);
}